{
  "compilerOptions": {
    "module": "system",
    "target": "es5",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "failOnTypeErrors": true,
    "sourceMap": true,
    "declaration": true,
	"stripInternal": true,
    "outFile": "dist/project.js"
  },
  "include": [
    "lib/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "grunt",
    "**/*.spec.ts",
    "demo"
  ]
}